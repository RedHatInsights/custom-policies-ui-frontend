{
  "name": "custom-policies-ui-front",
  "version": "1.1.0",
  "private": false,
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/stories/*"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "@patternfly/patternfly": "^2.40.9",
    "@patternfly/react-core": "^3.120.5",
    "@patternfly/react-icons": "^3.14.18",
    "@patternfly/react-table": "^2.24.22",
    "@redhat-cloud-services/frontend-components": "0.0.5",
    "@redhat-cloud-services/frontend-components-utilities": "0.0.10",
    "classnames": "^2.2.6",
    "csstips": "^1.2.0",
    "react": "^16.11.0",
    "react-content-loader": "^3.4.2",
    "react-dom": "^16.11.0",
    "react-flexview": "^4.0.4",
    "react-redux": "^5.1.2",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.1.1",
    "typestyle": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@redhat-cloud-services/frontend-components-config": "0.0.10",
    "@types/node": "11.11.4",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-fetch": "^3.0.4",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-react": "^7.16.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "prop-types": "^15.7.2",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-scss": "^3.12.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "build": "webpack --config config/prod.webpack.config.js",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --ext js,ts,tsx config src",
    "lint:js:fix": "eslint --ext js,ts,tsx config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.js",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "proxy": "SPANDX_CONFIG=\"./profiles/local-frontend.js\" bash $PROXY_PATH/scripts/run.sh"
  },
  "insights": {
    "appname": "custom-policies"
  }
}
