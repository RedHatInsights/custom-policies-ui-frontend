  "scripts": {
    "build": "webpack --config config/prod.webpack.config.ts",
    "test": "jest --verbose",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint --ext js,ts,tsx config src",
    "lint:ts:fix": "eslint --ext js,ts,tsx config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "prod": "NODE_ENV=production webpack-dev-server --config config/dev.webpack.config.ts",
    "server:ctr": "node src/server/generateServerKey.js",
    "start": "NODE_ENV=development webpack-dev-server --config config/dev.webpack.config.ts",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.ts",
    "verify": "npm-run-all build lint test",
    "nightly": "npm run deploy",
    "proxy": "SPANDX_CONFIG=\"profiles/local-frontend-and-api.ts\" bash $PROXY_PATH/scripts/run.sh",
    "proxy-ui": "SPANDX_CONFIG=\"profiles/local-frontend.ts\" bash $PROXY_PATH/scripts/run.sh",
    "schema": "yarn schema:download && yarn schema:generate && yarn schema:clean",
    "schema:download": "download http://localhost:8080/api/policies/v1.0/openapi.json --header 'Accept: application/json' -o utils/openapi-generator",
    "schema:generate": "bash ./.scripts/schema/generate.sh",
    "schema:clean": "rm -f ./utils/openapi-generator/openapi.json",
    "expression": "yarn expression:download && yarn expression:generate && yarn expression:copy && yarn expression:clean",
    "expression:download": "mkdir -p utils/expression; download https://raw.githubusercontent.com/RedHatInsights/policies-engine/master/api/src/main/antlr4/com/redhat/cloud/policies/api/model/condition/expression/parser/Expression.g4 -o utils/expression",
    "expression:generate": "antlr4ts -visitor utils/expression/Expression.g4",
    "expression:copy": "cp ./utils/expression/*.ts ./src/utils/Expression/; cp ./utils/expression/Expression.g4 ./src/utils/Expression",
    "expression:clean": "rm -rf ./utils/expression"
  },
  "insights": {
    "appname": "policies"
  }
}
